
material textures/effects/weapons/boomboom
{
	noselfShadow
	noshadows
	translucent
	discrete
	nonsolid
	noimpact
	{ 
		blend add
		map clamp textures/effects/weapons/boomboom
		vertexcolor
		maskAlpha
	}
}

