material textures/effects/weapons/darkmatter
{
	qer_editorimage textures/effects/weapons/darkmatteradd.tga
	noatmosphere
	translucent
	noshadows
	nonsolid
	twosided
	{
		blend add
		map clamp textures/effects/weapons/darkmatteradd.tga
		vertexColor
		rotate time * 0.6
	}
	{
		blend filter
		map clamp textures/effects/weapons/darkmatterfilter.tga
		rotate time * -0.6
	}
}

