material textures/decals/gore_splat/dsplat11
{
	qer_editorimage	textures/decals/gore_splat/dsplat11.tga

	nonsolid
	noimpact
	
	polygonOffset 1
	discrete
	sort decal
	noShadows
	
	{       
      	blend	gl_zero, gl_one_minus_src_color
		map clamp	textures/decals/gore_splat/dsplat11.tga
	}	
}


