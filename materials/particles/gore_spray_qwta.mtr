material textures/effects/gore_spray/bloodspray2
{
	qer_editorimage textures/effects/gore_spray/bloodspray2.tga
	polygonOffset 1
	discrete
	sort decal
	noShadows
	noShadows
	noimpact
	translucent

	{
		blend	blend
		Map textures/effects/gore_spray/bloodspray2.tga
		vertexColor
	}
}

