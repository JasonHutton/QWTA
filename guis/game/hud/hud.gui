				onPropertyChanged "deployableHealth" {
					if( deployableHealth > lastDeployableHealth ) {
						deployableOverrideColor = transition( "0,1,0,1", COLOR_WHITE, 300 );
					} else {
						deployableOverrideColor = transition( "1,0,0,1", COLOR_WHITE, 300 );
					}
