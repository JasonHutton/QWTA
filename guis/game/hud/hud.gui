	windowDef unconsciousFading {
		properties {
			color	foreColor		= COLOR_RED;
			rect	rect			= 0, 0, SCREEN_WIDTH, SCREEN_HEIGHT;
			float	selfUnconscious		= player.dead && player.spectator == false;
			float	specUnconscious		= player.localView == false && player.viewDead;
			float	selfDead		= player.limbo && player.spectator == false;
			float	unconsciousTime		= globals.gameHud.unconsciousTime;
			float	showFading		= gui.getCVarBool( "g_useDeathFading" );
			float	conditionals		= player.endGame == false && player.endGameCamera == false && showFading == true;
			float	visible 		= (selfUnconscious || specUnconscious || selfDead) && conditionals;
			float	tempValue		= 0;
		}
		events {
			onCreate {
				foreColor.a = 0;
			}
			onPreDraw {
				drawCachedMaterial( gui.wndFillMaterial, absoluteRect, foreColor );

				gui.scriptPushFloat( false );
			}
			onCVarChanged "g_useDeathFading" {
				showFading = gui.getCVarBool( "g_useDeathFading" );
			}
			onPropertyChanged "unconsciousTime" {
				if(unconsciousTime > -1) {
					tempValue = unconsciousTime/2.0;
					if(tempValue < 0.7) {
						foreColor.a = tempValue;
					}
					else {
						foreColor.a = 0.7;
					}
				}
				else {
					foreColor.a = 0;
				}
			}
		}
	}
	
	windowDef deadFading {
		properties {
